#include <stdio.h>


int isPrime(int num) {
    if (num <= 1) {
        return 0;
    }
    for (int i = 2; i * i <= num; i++) {
        if (num % i == 0) {
            return 0; 
        }
    }
    return 1;
}

int main() {
    int lowerBound, upperBound;

    printf("Enter the lower bound of the range: ");
    scanf("%d", &lowerBound);

    printf("Enter the upper bound of the range: ");
    scanf("%d", &upperBound);

    printf("Prime numbers between %d and %d are: ", lowerBound, upperBound);

    for (int i = lowerBound; i <= upperBound; i++) {
        if (isPrime(i)) {
            printf("%d ", i);
        }
    }
    printf("\n");

    return 0;
}
